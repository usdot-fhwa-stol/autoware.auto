<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>measurement_conversion</name>
  <version>1.0.0</version>
  <description>Adapts common message types into measurements for e.g. a Kalman filter</description>
  <maintainer email="opensource@apex.ai">Apex.AI, Inc</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>
  <buildtool_depend>autoware_auto_cmake</buildtool_depend>

  <depend>geometry_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>autoware_auto_common</depend>
  <depend>autoware_auto_msgs</depend>
  <depend>rclcpp</depend>
  <depend>eigen</depend>
  <depend>state_vector</depend>
  <depend>state_estimation</depend>
  <depend>tf2</depend>
  <depend>time_utils</depend>
  <depend>tf2_eigen</depend>
  <depend>tf2_geometry_msgs</depend>

  <test_depend>ament_cmake_gtest</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
    <build_depend>carma_cmake_common</build_depend>
</package>
